NODE_ENV=development
PORT=3000

# Database Configuration (SQLite)
DATABASE_URL=./src/data/namaste_demo.sqlite

# Legacy Database Configuration (if using PostgreSQL)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=namaste_demo
DB_USER=postgres
DB_PASS=password

# ABHA Configuration
ABHA_BASE_URL=https://dev.ndhm.gov.in/gateway
ABHA_CLIENT_ID=your_client_id_here
ABHA_CLIENT_SECRET=your_client_secret_here

# JWT Configuration
JWT_SECRET=your-jwt-secret-key

# FHIR Configuration
FHIR_BASE_URL=http://localhost:3000/fhir

# Demo Mode
DEMO_MODE=true

# Google Gemini AI Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_API_URL=https://generativelanguage.googleapis.com/v1beta/models/gemini-pro:generateContent

# Cache Configuration
REDIS_URL=redis://localhost:6379
CACHE_TTL=3600

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=./logs/combined.log
